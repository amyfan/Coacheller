<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<g:Label text="Loading..." horizontalAlignment="ALIGN_CENTER" ui:field="labelUpdating"/>
		<div align="center">
		<g:HorizontalPanel ui:field="loggedOutDisplay">
			<g:InlineLabel text="Login With: ["/>
			<g:InlineHyperlink text="Google" ui:field="linkGoogle"/>
			<g:InlineLabel text="] - ["/>
			<g:InlineHyperlink text="Facebook" ui:field="linkFacebook"/>
			<g:InlineLabel text="] - ["/>
			<g:InlineHyperlink text="Twitter"  ui:field="linkTwitter"/>
			<g:InlineLabel text="]"/>
		</g:HorizontalPanel>
		<g:HorizontalPanel ui:field="loggedInDisplay">
			<g:InlineLabel text="Hello -user name- ["/>
			<g:InlineHyperlink text="Log Out" targetHistoryToken="newHistoryToken" ui:field="linkLogOut"/>
			<g:InlineLabel text="]"/>
		</g:HorizontalPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 